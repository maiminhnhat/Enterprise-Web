<!doctype html>
<html lang="en" dir="ltr">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="icon" href="favicon.ico" type="image/x-icon" />
    <title><%- title %></title>
    <%- style %>
</head>

<body class="font-muli theme-cyan gradient right_tb_toggle">
    
    <!-- Page Loader -->
    <div class="page-loader-wrapper">
        <div class="loader">
        </div>
    </div>

    <div id="main_content">
        <!-- Start Main top header -->
        
        
       
        


        <!-- Start Main leftbar navigation MAIN MENU-->
        <div id="left-sidebar" class="sidebar">
            <h5 class="brand-name">Manager<a href="javascript:void(0)" class="menu_option float-right"><i class="icon-grid font-16" data-toggle="tooltip" data-placement="left" title="Grid & List Toggle"></i></a></h5>
            <ul class="nav nav-tabs">
                <li class="nav-item"><a id="menu-list-main" class="nav-link" data-toggle="tab" href="#menu-main">Main</a></li>
                <li class="nav-item"><a id="menu-list-account" class="nav-link" data-toggle="tab" href="#menu-account">Account</a></li>
            </ul>
            <div class="tab-content mt-3">
                <div class="tab-pane fade" id="menu-main" role="tabpanel">
                    <nav class="sidebar-nav">
                        <ul class="metismenu">
                            <li id="menu-list-home"><a href="/manager"><i class="fa fa-dashboard"></i><span>Dashboard</span></a></li>
                            <li id="menu-list-user"><a href="/manager/student_manage"><i class="fa fa-users"></i><span>Student</span></a></li>               
                            <li id="menu-list-faculty"><a href="/manager/faculty_manage"><i class="fa fa-building"></i><span>Faculty</span></a></li>
                            <li id="menu-list-topic"><a href="/manager/topic_manage"><i class="fa fa-graduation-cap"></i><span>Topic</span></a></li>
                            <li id="menu-list-contribution"><a href="/manager/contribution_manage"><i class="fa fa-book"></i><span>Contribution</span></a></li>
                            
                        </ul>
                    </nav>
                </div>
                <div class="tab-pane fade" id="menu-account" role="tabpanel">
                    <nav class="sidebar-nav">
                        <ul class="metismenu">

                            <li id="menu-list-profile"><a href="/manager/profile_manage"><i class="fa fa-list-ul"></i><span>Profile</span></a></li>
                            <li><a href="/staff/change_password"><i class="fa fa-unlock-alt"></i><span>Change password</span></a></li>
                            <li><a href="/logout"><i class="fa fa-power-off"></i><span>Log Out</span></a></li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
        
         <!-- Start project content area -->
    <div class="page">
      <!-- Start Page header -->
      <div class="section-body" id="page_top">
          <div class="container-fluid">
              <div class="page-header">
                  <div class="left">
                      <h1 style="color: #17a2b8;">Greenwich University</h1>
                  </div>
                  <div class="right">
                      <div class="notification d-flex">
                          
                          
                          
                          <div class="dropdown d-flex">
                              <a href="javascript:void(0)" class="chip ml-3" data-toggle="dropdown">
                                   <%= username %></a>
                              
                          </div>
                      </div>
                  </div>
              </div>
          </div>
      </div>

        <%- body %>

      </div>
        
      </div>
        <%- script %>
        <script src="../js/custom.js"></script>
        
      </body>
</html>