                                        <div class="gdlr-core-course-item-list ">
                                            <a class="gdlr-core-course-item-link" href=<%= files[index].id%>><span class="gdlr-core-course-item-id gdlr-core-skin-caption"><%= parseInt(index) + 1 %></span><span class="gdlr-core-course-item-title gdlr-core-skin-title"><%= files[index].file_name %></span><i class="gdlr-core-course-item-icon gdlr-core-skin-icon fa fa-long-arrow-right"></i></a>
                                        </div>
                                        <% }%>
                                    </div>
                                </div>                                                                                           
